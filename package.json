{
  "name": "pelive",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@payloadcms/admin-bar": "^3.76.1",
    "@payloadcms/db-vercel-postgres": "^3.76.1",
    "@payloadcms/email-resend": "^3.76.1",
    "@payloadcms/live-preview-react": "^3.76.1",
    "@payloadcms/next": "^3.76.1",
    "@payloadcms/plugin-form-builder": "^3.76.1",
    "@payloadcms/plugin-nested-docs": "^3.76.1",
    "@payloadcms/plugin-redirects": "^3.76.1",
    "@payloadcms/plugin-search": "^3.76.1",
    "@payloadcms/plugin-seo": "^3.76.1",
    "@payloadcms/richtext-lexical": "^3.76.1",
    "@payloadcms/storage-vercel-blob": "^3.76.1",
    "@payloadcms/ui": "^3.76.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@vercel/blob": "^2.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.4",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "0.563.0",
    "next": "^16.1.6",
    "next-sitemap": "^4.2.3",
    "payload": "^3.76.1",
    "prism-react-renderer": "^2.3.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "7.71.1",
    "react-icons": "^5.5.0",
    "resend": "^6.9.2",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "^10.4.19",
    "eslint": "^10.0.0",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "24.x",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
